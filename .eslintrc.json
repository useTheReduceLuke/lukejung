{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:import/typescript",
		"plugin:react/recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"ignorePatterns": [],
	"overrides": [
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"import",
		"react",
		"@typescript-eslint"
	],
	"rules": {
		"react/prop-types": 0,
		"import/no-unresolved": "error",
		"@typescript-eslint/ban-ts-comment": "off",
		"comma-dangle": [
			"error",
			"always-multiline"
		],
		"no-cond-assign": 2,
		"no-console": 1,
		"no-constant-condition": 2,
		"no-control-regex": 2,
		"no-debugger": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-empty": 2,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-invalid-regexp": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-sparse-arrays": 2,
		"no-unexpected-multiline": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-jsdoc": 2,
		"valid-typeof": 2,
		"no-extra-parens": "off",
		"no-irregular-whitespace": "off",
		"no-inner-declarations": [
			2,
			"functions"
		],
		"no-new-func": 1,
		"no-self-compare": 1,
		"dot-notation": 2,
		"eqeqeq": 2,
		"no-case-declarations": 2,
		"no-else-return": 2,
		"no-empty-pattern": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-fallthrough": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-magic-numbers": 0,
		"no-multi-spaces": 2,
		"no-multi-str": 2,
		"no-native-reassign": 2,
		"no-new-wrappers": 2,
		"no-new": 2,
		"no-octal-escape": 2,
		"no-octal": 2,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-self-assign": 2,
		"no-throw-literal": 2,
		"no-unmodified-loop-condition": 2,
		"no-useless-concat": 2,
		"no-with": 2,
		"accessor-pairs": 0,
		"no-return-assign": 0,
		"no-sequences": 0,
		"dot-location": [
			"error",
			"property"
		],
		"array-callback-return": 0,
		"block-scoped-var": 0,
		"complexity": 0,
		"consistent-return": 0,
		"curly": 0,
		"default-case": 0,
		"guard-for-in": 0,
		"no-alert": 0,
		"no-caller": 0,
		"no-div-regex": 0,
		"no-extend-native": 0,
		"no-extra-bind": 0,
		"no-extra-label": 0,
		"no-floating-decimal": 0,
		"no-implicit-coercion": 0,
		"no-implicit-globals": 0,
		"no-implied-eval": 0,
		"no-invalid-this": 0,
		"no-iterator": 0,
		"no-labels": 0,
		"no-param-reassign": 0,
		"no-process-env": 0,
		"no-script-url": 0,
		"no-unused-expressions": 0,
		"no-unused-labels": 0,
		"no-useless-call": 0,
		"no-void": 0,
		"no-warning-comments": 0,
		"radix": 0,
		"vars-on-top": 0,
		"wrap-iife": 0,
		"yoda": 0,
		"arrowParens": 0,
		"no-delete-var": 2,
		"no-shadow-restricted-names": 2,
		"no-shadow": 2,
		"no-undef-init": 2,
		"no-undef": 2,
		"no-undefined": 2,
		"no-use-before-define": 2,
		"no-unused-vars": [
			1,
			{
				"vars": "local",
				"args": "after-used"
			}
		],
		"init-declarations": 0,
		"no-catch-shadow": 0,
		"no-label-var": 0,
		"callback-return": 0,
		"global-require": 0,
		"handle-callback-err": 0,
		"no-mixed-requires": 0,
		"no-new-require": 0,
		"no-path-concat": 0,
		"no-process-exit": 0,
		"no-restricted-imports": 0,
		"no-restricted-modules": 0,
		"no-sync": 0,
		"no-nested-ternary": 1,
		"eol-last": 2,
		"newline-per-chained-call": 2,
		"no-array-constructor": 2,
		"no-bitwise": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-negated-condition": 2,
		"no-new-object": 2,
		"no-whitespace-before-property": 2,
		"no-trailing-spaces": 2,
		"semi-spacing": 2,
		"sort-vars": 2,
		"sort-imports": 0,
		"space-before-blocks": 2,
		"space-infix-ops": 2,
		"space-unary-ops": 2,
		"brace-style": "error",
		"comma-style": "error",
		"max-statements": [
			"error",
			30
		],
		"max-depth": [
			"error",
			5
		],
		"max-params": [
			"error",
			5
		],
		"newline-after-var": 0,
		"object-curly-spacing": [
			"error",
			"always"
		],
		"semi": "error",
		"spaced-comment": [
			"error",
			"always"
		],
		"computed-property-spacing": [
			"error",
			"always"
		],
		"space-in-parens": 1,
		"func-call-spacing": [
			"error",
			"never"
		],
		"space-before-function-paren": [
			"error",
			"never"
		],
		"comma-spacing": [
			"error",
			{
				"before": false,
				"after": true
			}
		],
		"keyword-spacing": [
			"error",
			{
				"before": true,
				"after": true
			}
		],
		"lines-around-comment": [
			"error",
			{
				"beforeBlockComment": true
			}
		],
		"max-nested-callbacks": [
			"error",
			3
		],
		"indent": [
			"error",
			4
		],
		"max-len": [
			"error",
			{
				"code": 100,
				"ignoreComments": true,
				"ignoreTemplateLiterals": true,
				"ignoreUrls": true,
				"ignoreStrings": true
			}
		],
		"key-spacing": [
			"error",
			{
				"mode": "minimum",
				"beforeColon": false,
				"afterColon": true,
				"align": "value"
			}
		],
		"block-spacing": "error",
		"array-bracket-spacing": [
			"error",
			"always",
			{
				"objectsInArrays": false,
				"arraysInArrays": false
			}
		],
		"array-element-newline": ["error", {
			"ArrayExpression": "consistent",
			"ArrayPattern": { "minItems": 4 }
		}],
		"camelcase": 0,
		"consistent-this": 0,
		"func-names": 0,
		"func-style": 0,
		"id-length": 0,
		"id-match": 0,
		"id-blacklist": 0,
		"jsx-quotes": 0,
		"linebreak-style": 0,
		"new-cap": 0,
		"new-parens": 0,
		"no-continue": 0,
		"no-inline-comments": 0,
		"no-lonely-if": 0,
		"no-multiple-empty-lines": 0,
		"no-plusplus": 0,
		"no-restricted-syntax": 0,
		"no-spaced-func": 0,
		"no-ternary": 0,
		"no-underscore-dangle": 0,
		"no-unneeded-ternary": 0,
		"one-var": 0,
		"one-var-declaration-per-line": 0,
		"operator-assignment": 0,
		"operator-linebreak": 0,
		"padded-blocks": 0,
		"quote-props": 0,
		"quotes": 0,
		"require-jsdoc": 0,
		"wrap-regex": 0,
		"no-const-assign": 2,
		"no-dupe-class-members": 2,
		"no-var": 2,
		"no-useless-constructor": 2,
		"prefer-arrow-callback": 2,
		"prefer-const": 2,
		"prefer-rest-params": 2,
		"prefer-spread": 2,
		"object-shorthand": [
			"error",
			"always",
			{
				"avoidQuotes": true
			}
		],
		"template-curly-spacing": [
			"error",
			"always"
		],
		"rest-spread-spacing": [
			"error",
			"never"
		],
		"arrow-spacing": 2,
		"arrow-body-style": 0,
		"arrow-parens": 0,
		"constructor-super": 0,
		"generator-star-spacing": 0,
		"no-class-assign": 0,
		"no-confusing-arrow": 0,
		"no-new-symbol": 0,
		"no-this-before-super": 0,
		"prefer-reflect": 0,
		"prefer-template": 0,
		"require-yield": 0,
		"yield-star-spacing": 0
	},
	"settings": {
		"import/parsers": {
			"@typescript-eslint/parser": [".ts", ".tsx"]
		},
		"import/resolver": {
			"typescript": {
				"alwaysTryTypes": true // always try to resolve types under `<root>@types` directory even it doesn't contain any source code, like `@types/unist`
			},
			"node": {
				"extensions": [".js", ".jsx", ".ts", ".tsx"]
			}
		}
	}
}
